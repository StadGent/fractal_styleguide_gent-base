{% extends "_form-item--fieldset.template.twig" %}

{% block options %}
  {% for option in options %}
    {% include '@'~input_component with {
      "id": input_component ~ "-" ~ id ~ "-" ~ modifier ~ "-" ~ option.id,
      "type": type,
      "name": option.name,
      "label": option.label
    } %}
  {% endfor %}
{% endblock %}
